            @functions {
                // Render the label and its associated modules
                void RenderLabelAndModules(ModuleDto label, IEnumerable<ModuleDto> allModules, string currentUrl)
                {
                    if (!string.IsNullOrEmpty(label.LabelText))
                    {
                        <span class="dropdown-header mt-4">@label.LabelText</span>
                        <small class="bi-three-dots nav-subtitle-replacer"></small>
                    }

                    // Get all modules under this label, ordered by "Order"
                    var modulesUnderLabel = allModules
                    .Where(m => m.ParentModuleId == label.Id && m.IsModule)
                    .OrderBy(m => m.Order)
                    .ToList();

                    foreach (var module in modulesUnderLabel)
                    {
                        RenderModuleWithChildren(module, allModules, currentUrl);
                    }
                }

                // Recursive function to render modules and their children
                void RenderModuleWithChildren(ModuleDto module, IEnumerable<ModuleDto> allModules, string currentUrl)
                {
                    var childModules = allModules
                    .Where(m => m.ParentModuleId == module.Id)
                    .OrderBy(m => m.Order)
                    .ToList();

                    var moduleUrl = module.ModuleUrl.StartsWith("/") ? module.ModuleUrl : "/" + module.ModuleUrl;
                    var hasChildren = childModules.Any();
                    var isActive = currentUrl == moduleUrl || childModules.Any(m => currentUrl.Contains(m.ModuleUrl));

                    <div class="nav-item">
                        @if (hasChildren)
                        {
                            <!-- Parent module with independent dropdown -->
                            <a class="nav-link dropdown-toggle @(isActive ? "active" : "")" href="#menu_@module.Id"
                               role="button" data-bs-toggle="collapse"
                               data-bs-target="#menu_@module.Id" aria-expanded="@(isActive ? "true" : "false")"
                               aria-controls="menu_@module.Id">
                                <i class="@module.ModuleIcon nav-icon"></i>
                                <span class="nav-link-title">@module.ModuleName</span>
                            </a>
                            <div id="menu_@module.Id" class="nav-collapse collapse @(isActive ? "show" : "")">
                                @foreach (var child in childModules)
                                {
                                    RenderModuleWithChildren(child, allModules, currentUrl);
                                }
                            </div>
                        }
                        else
                        {
                            <!-- Leaf module -->
                            <a class="nav-link @(isActive ? "active" : "")" href="@moduleUrl">
                                <i class="@module.ModuleIcon nav-icon"></i>
                                <span class="nav-link-title">@module.ModuleName</span>
                            </a>
                        }
                    </div>
                }
            }









