@{
    // ---------------------------------------
    // 1) Retrieve the current year/month/week
    // ---------------------------------------
    var currentYear = DateTime.Now.Year;
    var currentMonth = DateTime.Now.Month;

    // Example of calculating "week number" for the current date
    int currentWeekNumber = CultureInfo.CurrentCulture.Calendar.GetWeekOfYear(
        DateTime.Now,
        CalendarWeekRule.FirstFourDayWeek,
        DayOfWeek.Monday);

    // ---------------------------------------
    // 2) Pull the relevant data from the model
    // ---------------------------------------
    // For weekly
    decimal weeklyProfit = 0;
    var currentWeekPL = Model.WeeklyProfitLoss
        .FirstOrDefault(x => x.Year == currentYear
                       /* and if your DTO includes a "WeekNumber": */
                       // && x.WeekNumber == currentWeekNumber
                       /* or if your SP stores it differently, adapt accordingly */
                       );
    if (currentWeekPL != null)
    {
        weeklyProfit = currentWeekPL.ProfitOrLoss;
    }

    // For monthly
    decimal monthlyProfit = 0;
    var currentMonthPL = Model.MonthlyProfitLoss
        .FirstOrDefault(x => x.Year == currentYear && x.MonthNumber == currentMonth);
    if (currentMonthPL != null)
    {
        monthlyProfit = currentMonthPL.ProfitOrLoss;
    }

    // For yearly
    decimal yearlyProfit = 0;
    var currentYearPL = Model.YearlyProfitLoss
        .FirstOrDefault(x => x.Year == currentYear);
    if (currentYearPL != null)
    {
        yearlyProfit = currentYearPL.ProfitOrLoss;
    }

    // ---------------------------------------
    // 3) Define "target" values for each scope
    //    (Could come from config, DB, or logic)
    // ---------------------------------------
    decimal weeklyTarget = 2000m;     // e.g. $2K weekly target
    decimal monthlyTarget = 10000m;   // e.g. $10K monthly target
    decimal yearlyTarget = 120000m;   // e.g. $120K yearly target

    // ---------------------------------------
    // 4) Calculate percentages & clamp (0-100)
    // ---------------------------------------
    decimal weeklyPercent = 0;
    if (weeklyTarget > 0)
        weeklyPercent = (weeklyProfit / weeklyTarget) * 100;
    weeklyPercent = Math.Clamp(weeklyPercent, 0, 100);

    decimal monthlyPercent = 0;
    if (monthlyTarget > 0)
        monthlyPercent = (monthlyProfit / monthlyTarget) * 100;
    monthlyPercent = Math.Clamp(monthlyPercent, 0, 100);

    decimal yearlyPercent = 0;
    if (yearlyTarget > 0)
        yearlyPercent = (yearlyProfit / yearlyTarget) * 100;
    yearlyPercent = Math.Clamp(yearlyPercent, 0, 100);
}



@{
    /****************************************************************
     *  CHART #1: BAR CHART
     *  "Sales & Purchases (Bar)"
     *  We'll show the last 6 months from MonthlySales & MonthlyPurchase
     ****************************************************************/
    var last6MonthsSales = Model.MonthlySales
        // Sort descending (most recent first)
        .OrderByDescending(m => m.Year)
        .ThenByDescending(m => m.MonthNumber)
        // Take the last 6
        .Take(6)
        // Then re-order ascending so oldest is first
        .OrderBy(m => m.Year)
        .ThenBy(m => m.MonthNumber)
        .ToList();

    // Build label array from MonthName
    var barChartLabels = last6MonthsSales.Select(s => s.MonthName).ToArray();

    // Build Sales data array
    var barChartSalesData = last6MonthsSales.Select(s => s.TotalSales).ToArray();

    // Gather matching months for Purchase
    // (You could also do separate logic if you need exactly 6 months from purchase side)
    var last6MonthsPurchase = Model.MonthlyPurchase
        .Where(p => last6MonthsSales.Any(s => s.Year == p.Year && s.MonthNumber == p.MonthNumber))
        .ToList();

    // Build Purchase data array, matching the same months from last6MonthsSales
    var barChartPurchaseData = last6MonthsSales.Select(s =>
    {
        var match = last6MonthsPurchase
            .FirstOrDefault(p => p.Year == s.Year && p.MonthNumber == s.MonthNumber);
        return match?.TotalPurchase ?? 0;
    }).ToArray();


    /****************************************************************
     *  CHART #2: DOUGHNUT CHART
     *  "Sales Distribution (Doughnut) - Last 4 Months"
     *  We'll simply show the last 4 months from MonthlySales
     ****************************************************************/
    var last4Months = Model.MonthlySales
        .OrderByDescending(x => x.Year)
        .ThenByDescending(x => x.MonthNumber)
        .Take(4)
        .OrderBy(x => x.Year)
        .ThenBy(x => x.MonthNumber)
        .ToList();

    // The labels & data will be these 4 months
    var doughnutChartLabels = last4Months.Select(x => x.MonthName).ToArray();
    var doughnutChartData = last4Months.Select(x => x.TotalSales).ToArray();


    /****************************************************************
     *  CHART #3: LINE CHART
     *  "Daily Sales (Line) - Last 7 Days"
     *  We show the last 7 entries from DailySales
     ****************************************************************/
    var last7Days = Model.DailySales
        .OrderByDescending(d => d.Date)  // Date is a string "yyyy-MM-dd"
        .Take(7)
        .OrderBy(d => d.Date)
        .ToList();

    // For labels, parse the Date string into short date (e.g. "Feb 21")
    var lineChartLabels = last7Days.Select(d =>
    {
        DateTime dt;
        if (DateTime.TryParse(d.Date, out dt))
            return dt.ToString("MMM dd");
        return d.Date; // fallback if parse fails
    }).ToArray();

    var lineChartData = last7Days.Select(d => d.TotalSales).ToArray();


    /****************************************************************
     *  CHART #4: RADAR CHART
     *  "Sales vs. Purchases (Radar) - Last 5 Months"
     ****************************************************************/
    var last5MonthsRadar = Model.MonthlySales
        .OrderByDescending(m => m.Year)
        .ThenByDescending(m => m.MonthNumber)
        .Take(5)
        .OrderBy(m => m.Year)
        .ThenBy(m => m.MonthNumber)
        .ToList();

    var radarChartLabels = last5MonthsRadar.Select(m => m.MonthName).ToArray();
    var radarChartSalesData = last5MonthsRadar.Select(m => m.TotalSales).ToArray();

    // Matching data for purchases
    var last5MonthsRadarPurchases = Model.MonthlyPurchase
        .Where(p => last5MonthsRadar.Any(s => s.Year == p.Year && s.MonthNumber == p.MonthNumber))
        .ToList();
    var radarChartPurchaseData = last5MonthsRadar.Select(m =>
    {
        var match = last5MonthsRadarPurchases
            .FirstOrDefault(p => p.Year == m.Year && p.MonthNumber == m.MonthNumber);
        return match?.TotalPurchase ?? 0;
    }).ToArray();


    /****************************************************************
     *  CHART #5: POLAR AREA CHART
     *  "Sales by Year (Polar) - Last 5 Years"
     ****************************************************************/
    var last5YearsSales = Model.YearlySales
        .OrderByDescending(y => y.Year)
        .Take(5)
        .OrderBy(y => y.Year)
        .ToList();

    var polarChartLabels = last5YearsSales.Select(y => y.Year.ToString()).ToArray();
    var polarChartData = last5YearsSales.Select(y => y.TotalSales).ToArray();


    /****************************************************************
     *  CHART #6: PIE CHART
     *  "Purchases by Year (Pie) - Last 3 Years"
     ****************************************************************/
    var last3YearsPurchase = Model.YearlyPurchase
        .OrderByDescending(y => y.Year)
        .Take(3)
        .OrderBy(y => y.Year)
        .ToList();

    var pieChartLabels = last3YearsPurchase.Select(y => y.Year.ToString()).ToArray();
    var pieChartData = last3YearsPurchase.Select(y => y.TotalPurchase).ToArray();
}

